@model Models.Edificio

@{
    ViewBag.Title = "Details";
}
<section class="content-header">
    <h2>Detalles</h2>
</section>

<section class="content">

    <div class="box">
        <div class="box-body">
            <div>
                <h4>Edificio</h4>
                <hr />
                <dl class="dl-horizontal">
                    <dt>
                        @Html.DisplayName("Nombre")
                    </dt>
                    <dd>
                        @Html.DisplayFor(model => model.nombre_edificio)
                    </dd>

                    <dt>
                        @Html.DisplayName("Observación")
                    </dt>
                    <dd>
                        @Html.DisplayFor(model => model.observacion)
                    </dd>

                    <dt>
                        @Html.DisplayName("Estado")
                    </dt>
                    <dd>
                        @Html.DisplayFor(model => model.estado_edificio)
                    </dd>
                </dl>
            </div>
            <p>
                @Html.ActionLink("Edit", "Edit", new { id = Model.EdificioId }) |
                @Html.ActionLink("Back to List", "Index")
            </p>



        </div>
    </div>

</section>
