@model IEnumerable<Models.Edificio>

@{
    ViewBag.Title = "Index";
}

<h2>Index</h2>

<p>
    @Html.ActionLink("Create New", "Create")
</p>
<table class="table">
    <tr>
        <th>
            @Html.DisplayNameFor(model => model.nombre_edificio)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.observacion)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.estado_edificio)
        </th>
        <th></th>
    </tr>

@foreach (var item in Model) {
    <tr>
        <td>
            @Html.DisplayFor(modelItem => item.nombre_edificio)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.observacion)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.estado_edificio)
        </td>
        <td>
            @Html.ActionLink("Edit", "Edit", new { id=item.EdificioId }) |
            @Html.ActionLink("Details", "Details", new { name = item.nombre_edificio }) |
            @Html.ActionLink("Delete", "Delete", new { id=item.EdificioId })
        </td>
    </tr>
}

</table>
