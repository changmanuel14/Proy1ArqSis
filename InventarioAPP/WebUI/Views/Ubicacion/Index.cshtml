@model IEnumerable<Models.Ubicacion>

@{
    ViewBag.Title = "Index";
}

<h2>Index</h2>

<p>
    @Html.ActionLink("Create New", "Create")
</p>
<table class="table">
    <tr>
        <th>
            @Html.DisplayNameFor(model => model.EdificioId)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.nombre_ubicacion)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.observacion)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.estado_ubicacion)
        </th>
        <th></th>
    </tr>

@foreach (var item in Model) {
    <tr>
        <td>
            @Html.DisplayFor(modelItem => item.EdificioId)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.nombre_ubicacion)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.observacion)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.estado_ubicacion)
        </td>
        <td>
            @Html.ActionLink("Edit", "Edit", new { id=item.UbicacionId }) |
            @Html.ActionLink("Details", "Details", new { name = item.nombre_ubicacion }) |
            @Html.ActionLink("Delete", "Delete", new { id=item.UbicacionId })
        </td>
    </tr>
}

</table>
