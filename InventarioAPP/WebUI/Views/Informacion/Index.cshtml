@model IEnumerable<Models.informacion1>

@{
    ViewBag.Title = "Index";
}
<section class="content-header">
    <h2>Información 1</h2>
</section>

<section class="content">

    <div class="box">
        <div class="box-body">
            <table class="table">
                <tr>
                    <th>
                        @Html.DisplayName("Edificio")
                    </th>
                    <th>
                        @Html.DisplayName("Ubicación")
                    </th>
                    <th>
                        @Html.DisplayName("Categoría")
                    </th>
                    <th>
                        @Html.DisplayName("Tipo de Mobiliario")
                    </th>
                    <th>
                        @Html.DisplayName("Descripción")
                    </th>
                    <th></th>
                </tr>

                @foreach (var item in Model)
                {
                <tr>
                    <td>
                        @Html.DisplayFor(modelItem => item.nombre_edificio)
                    </td>
                    <td>
                        @Html.DisplayFor(modelItem => item.nombre_ubicacion)
                    </td>
                    <td>
                        @Html.DisplayFor(modelItem => item.nombre_categoria)
                    </td>
                    <td>
                        @Html.DisplayFor(modelItem => item.DescripcionTM)
                    </td>
                    <td>
                        @Html.DisplayFor(modelItem => item.descripcion_mobiliario)
                    </td>
                </tr>
                }

            </table>
        </div>
    </div>

</section>

